def avg(n1,n2,n3):
	return(n1+n2+n3)/3.0
lol=avg(10,30,40)
print(lol)

def func(*t):
    print(t)                        
    print(t[7])

def func2(**d):
    print(d)

def fun3(a,b,c=3):
    return(a+b+c)
result=fun3(10,20)
print(result)
    
func(1,2,3,4,5,6,7,"lol")

func2(a=1,b=2,c=[4,5,6],f="ABCD",g=89.6)

